func alloc<T>(value: T): *T {
	const allocation = malloc(size<T>() as i64) as *T
	*allocation = value
	return allocation
}

func (self: *T) free<T>() {
	free(self as unknown)
}

func `+`(left: str, right: str): str {
	var result = new [chr, strlen(left)+1] { } as str
	strcpy(result, left)
	strcat(result, right)
	return result
}